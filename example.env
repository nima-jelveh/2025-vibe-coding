# Example environment variables for local development
# Copy this file to .env and fill in your actual values

# Lakebase Configuration
LAKEBASE_INSTANCE_NAME='your-lakebase-instance-name'
LAKEBASE_DB_NAME='your_database_name'

# Databricks Authentication (for local development only)
# When deployed to Databricks Apps, these are not needed
DATABRICKS_CLIENT_ID='your-client-id'
DATABRICKS_CLIENT_SECRET='your-client-secret'
DATABRICKS_HOST='https://your-workspace.cloud.databricks.com'

# Your email for local testing
MY_EMAIL='your.email@example.com'
